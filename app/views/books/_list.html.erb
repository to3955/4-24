<h2>User info</h2>
 <%= image_tag user.get_profile_image(100,100) %>
 <table class="table">
  <tbody>
   <tr></tr>
   <tr>
    <th>names</th>
    <th><%= user.name %></th>
   </tr> 
   <tr>
    <th>introduction</th>
    <th><%= user.introduction %></th>
    </tr>
    </tbody>
  </table>
    
   <p><%= link_to "プロフィール編集", edit_user_path(current_user.id) %></p>



<h1>New book</h1>
<%= form_with model: book do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Opinion</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>

