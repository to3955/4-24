<h2>User info</h2>

<div>
  <%= image_tag user.get_profile_image(100,100) %>
    <tr>
    <td>names</td>
    <td>introduction</td>
    </tr>
    
   <p><%= link_to "プロフィール編集", edit_user_path(current_user.id) %></p>
</div>


<h1>New book</h1>
<%= form_with model: book do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Opinion</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>

